# Example terraform.tfvars file
# Copy this to terraform.tfvars and update with your actual values

aws_region = "us-east-1"
environment = "prod"

# Your domain name
domain_name = "kingslanding.io"

# SSL certificate ARN (must be in us-east-1 for CloudFront)
# Get this from AWS Certificate Manager
certificate_arn = "arn:aws:acm:us-east-1:123456789012:certificate/12345678-1234-1234-1234-123456789012"

# Optional: If you want to import existing Cognito resources
# cognito_user_pool_id = "us-east-1_XXXXXXXXX"
# cognito_identity_pool_id = "us-east-1:12345678-1234-1234-1234-123456789012"

# Optional: If you want to import existing API Gateway
# api_gateway_id = "abcdefghij"